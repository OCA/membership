<openerp>
<data>

<record id="decline_reason_wizard_form" model="ir.ui.view">
    <field name="name">Decline membership</field>
    <field name="model">membership.decline_reason_wizard</field>
    <field name="arch" type="xml">
        <form string="Decline membership">
            <group>
                <field name="date_decline"/>
                <field name="date_to"/>
                <field name="decline_reason"/>
            </group>
            <footer>
                <button string="Apply" name="decline"
                        type="object" class="oe_highlight"/>
                or
                <button string="Cancel" class="oe_link" special="cancel" />
            </footer>
        </form>
    </field>
</record>

</data>
</openerp>
